{
  "Oidc": {
    "ProviderOptions": {
      "ClientId": "client.customer",
      "Authority": "https://localhost:5001",
      "ResponseType": "code",
      "ResponseMode": "fragment"
    },
    "AuthenticationPaths": {
      "RemoteRegisterPath": "https://localhost:5001/Account/Register",
      "RemoteProfilePath": "https://localhost:5001/Account/Manage"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "BrowserConsole",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Properties": { "Application": "Doitsu.Ecommerce.Presentation.Client" }
  }
}